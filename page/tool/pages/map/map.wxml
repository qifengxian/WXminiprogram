<view class="container">
  <view class="page-body">
    <view class="page-section page-section-gap">
			<map
			scale="17"
			style="width: 100%; height: 1050rpx;"
			latitude="{{latitude}}"
			longitude="{{longitude}}"
			markers="{{marker}}"
			>
      </map>
			<button class="page-body-button" type="primary" bindtap="getLocation">定位</button>
    </view>
  </view>
</view>
